<!DOCTYPE html>
<html lang="{{ app.request.locale }}" ng-app="myApp">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1" />
    <title>{% block title %}Welcome!{% endblock %}SecureBook</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    <link rel="stylesheet" href="{{ asset('assets/external/libs/font-awesome/css/font-awesome.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/external/libs/bootstrap/dist/css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/external/libs/bootstrap/dist/css/bootstrap-theme.min.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/app/css/utility.css') }}">
    <link rel="stylesheet" href="{{ asset('assets/app/css/app.css') }}">
    {% if not is_granted('ROLE_USER') %}
        <link rel="stylesheet" href="{{ asset('bundles/sbuser/css/login_register.css') }}">
    {% endif %}
    {% block stylesheets %}{% endblock %}
    <script src="{{ asset('assets/external/libs/jquery/dist/jquery.min.js') }}"></script>
    <script src="{{ asset('assets/external/libs/bootstrap/dist/js/bootstrap.min.js') }}"></script>
    {% block javascripts %}{% endblock %}
</head>
<body class="ng-scope" ng-controller="mainController">
{% block content %}{% endblock %}
</body>
</html>
