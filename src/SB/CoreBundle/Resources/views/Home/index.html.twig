{% extends 'SBCoreBundle::layout.html.twig' %}

{% block body %}
    <div class="container">
        <div class="row">
            {% if list_activity is defined %}
                {% for activity in list_activity %}
                    {{ dump(activity) }}
                {% endfor %}
            {% endif %}
        </div>
        <div class="row">
            {% if user is defined %}
                {{ dump(user) }}
            {% endif %}
        </div>
    </div>
{% endblock body %}